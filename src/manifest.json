{
  "name": "Cookie助手",
  "description": "获取网站cookie信息",
  "version": "1.0.0",
  "permissions": [
    "contextMenus",
    "tabs",
    "storage",
    "webNavigation",
    "notifications",
    "activeTab",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [
    ]
  },
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_title": "Cookie助手",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}

